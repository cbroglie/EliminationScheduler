<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:com.cbroglie.eliminationscheduler.client.ui">

	<ui:style type='com.cbroglie.eliminationscheduler.client.view.MyPicksView.Style'>
		.selections {
			font-size: 1em;
			color: #777777;
		}

		.entries {
			font-size: 1em;
			color: #777777;
		}
	</ui:style>

	<g:DecoratorPanel width="100%">
		<g:VerticalPanel>
			<my:SelectOneListBox ui:field="entriesListBox" width="100%" addStyleNames="{style.entries}"/>
			<g:FlexTable ui:field="selectionsTable" width="100%"/>
		</g:VerticalPanel>
	</g:DecoratorPanel>
</ui:UiBinder> 